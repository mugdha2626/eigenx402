{
  "name": "eigenx402-widget",
  "version": "1.0.0",
  "description": "Payment gateway widgets with x402 protocol + EigenCompute verification. Drop-in crypto payments for any website.",
  "main": "dist/widget.js",
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:widget && npm run build:button",
    "build:widget": "esbuild src/widget.ts --bundle --minify --outfile=dist/widget.js --format=iife --global-name=EigenX402Widget",
    "build:button": "esbuild src/button-widget.ts --bundle --minify --outfile=dist/button-widget.js --format=iife --global-name=EigenX402Button",
    "dev": "npm run build:widget -- --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "payment",
    "gateway",
    "widget",
    "crypto",
    "web3",
    "x402",
    "eigencompute",
    "usdc",
    "ethereum",
    "base",
    "verifiable-compute",
    "tee"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mugdha2626/eigenx402.git",
    "directory": "packages/widget"
  },
  "homepage": "https://github.com/mugdha2626/eigenx402#readme",
  "bugs": {
    "url": "https://github.com/mugdha2626/eigenx402/issues"
  },
  "author": "mugdha2626",
  "license": "MIT",
  "dependencies": {
    "@eigenx402/client-sdk": "^1.0.0",
    "@eigenx402/types": "^1.0.0",
    "ethers": "^6.10.0"
  },
  "devDependencies": {
    "esbuild": "^0.19.11",
    "typescript": "^5.3.3"
  }
}
