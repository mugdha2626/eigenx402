{
  "name": "tee-service-template",
  "version": "1.0.0",
  "description": "Generic TEE service template with x402 payment protection",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "docker:build": "docker build --platform linux/amd64 -t my-tee-service:latest .",
    "docker:run": "docker run -p 8080:8080 my-tee-service:latest"
  },
  "dependencies": {
    "@eigenx402/server-sdk": "file:../../packages/server-sdk",
    "@eigenx402/types": "file:../../packages/types",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2"
  }
}
