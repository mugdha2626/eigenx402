# Network Configuration
BASE_RPC_URL=https://sepolia.base.org
NETWORK=base-sepolia
CHAIN_ID=84532

# USDC Token (Base Sepolia)
USDC_ADDRESS=0x036CbD53842c5426634e7929541eC2318f3dCF7e

# Merchant Configuration
MERCHANT_WALLET=0xYourMerchantWalletAddress
MERCHANT_PRIVATE_KEY=0xYourPrivateKeyForSigning

# Payment Configuration
PRICE_USDC=0.05
# Set to 'true' to enable REAL on-chain USDC transfers (costs gas!)
ENABLE_REAL_SETTLEMENT=false
MAX_TIMEOUT_SECONDS=3600

# EigenCompute Configuration
COMPUTE_APP_URL=https://your-app.eigencloud.xyz
EIGEN_IMAGE_DIGEST=sha256:your-image-digest-here

# Deterministic Inference Configuration
# EigenAI Models: gpt-oss-120b-f16, qwen3-32b-128k-bf16
# EigenAI provides verifiable AI with cryptographic signatures
MODEL_ID=gpt-oss-120b-f16
DEFAULT_SEED=42

# Inference Configuration
# demo = Seeded random text (no API key needed)
# production = Real EigenAI with verifiable compute
INFERENCE_MODE=production

# EigenAI API Key (Required for production mode)
# Get from: https://eigencloud.xyz (contact team for onboarding)
# Testnet: https://eigenai-sepolia.eigencloud.xyz
# Mainnet: https://eigenai.eigencloud.xyz
EIGENAI_API_KEY=your_eigenai_api_key_here

# Database
DATABASE_URL=file:./dev.db

# Server Configuration
SERVER_PORT=3001
SERVER_HOST=localhost

# Facilitator (optional - defaults to x402.org)
FACILITATOR_URL=https://x402.org/facilitator
