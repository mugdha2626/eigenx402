# Server Configuration
PORT=8080
NODE_ENV=production

# Payment Configuration
PRICE_USDC=0.05
MERCHANT_WALLET=0xYourMerchantWalletAddress
MERCHANT_PRIVATE_KEY=0xYourPrivateKey

# Network Configuration
NETWORK=base-sepolia
USDC_ADDRESS=0x036CbD53842c5426634e7929541eC2318f3dCF7e
CHAIN_ID=84532
BASE_RPC_URL=https://sepolia.base.org

# Payment Settlement
# Set to 'true' for production (real on-chain settlement)
# Set to 'false' for demo (simulated transactions)
ENABLE_REAL_SETTLEMENT=false

# EigenCompute TEE Configuration
# These are set automatically when deployed to EigenCompute
EIGEN_IMAGE_DIGEST=sha256:local-dev
EIGEN_ATTESTATION_URL=

# Optional: External API Keys (if needed for your service)
# OPENAI_API_KEY=
# ANTHROPIC_API_KEY=
# EIGENAI_API_KEY=
# GEMINI_API_KEY=
